#$(MONOLITHLIB_FILES): INCLUDES += -i libs/monolithlib/include/ -i libs/mm/include/ -i libs/nw4r/include/
$(MONOLITHLIB_FILES): CFLAGS += -ipa file -inline auto -str pool,readonly,reuse -RTTI on -enc SJIS

DEPENDS += $(MONOLITHLIB_FILES:.o=.d)

$(BUILD_DIR)/libs/monolithlib/monolithlib.a: $(MONOLITHLIB_FILES)
	@echo Linking... $@
	$(QUIET) mkdir -p $(dir $@)
	@echo $(MONOLITHLIB_FILES) > build/MONOLITHLIB_ofiles
	$(QUIET) $(LD) -library $(LIBRARY_LDFLAGS) -o $@ -lcf ldscript.lcf @build/MONOLITHLIB_ofiles
