#$(MONOLIB_FILES): INCLUDES += -i libs/monolib/include/ -i libs/nw4r/include/
$(MONOLIB_FILES): CFLAGS += -ipa file -inline auto -str pool,readonly,reuse -RTTI on -enc SJIS

DEPENDS += $(MONOLIB_FILES:.o=.d)

$(BUILD_DIR)/libs/monolib/monolib.a: $(MONOLIB_FILES)
	@echo Linking... $@
	$(QUIET) mkdir -p $(dir $@)
	@echo $(MONOLIB_FILES) > build/MONOLIB_ofiles
	$(QUIET) $(LD) -library $(LIBRARY_LDFLAGS) -o $@ -lcf ldscript.lcf @build/MONOLIB_ofiles
