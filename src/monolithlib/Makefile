$(MONOLITHLIB_FILES): CFLAGS += -ipa file -str pool,readonly,reuse -RTTI on -enc SJIS

DEPENDS += $(MONOLITHLIB_FILES:.o=.d)

$(BUILD_DIR)/src/monolithlib/monolithlib.a: $(MONOLITHLIB_FILES)
	@echo Linking... $@
	$(QUIET) mkdir -p $(dir $@)
	@echo $(MONOLITHLIB_FILES) > build/MONOLITHLIB_ofiles
	$(QUIET) $(LD) -library $(LIBRARY_LDFLAGS) -o $@ -lcf ldscript.lcf @build/MONOLITHLIB_ofiles